{
    "name": "forkcms/forkcms",
    "description": "Fork is an open source CMS that will rock your world.",
    "keywords": ["cms"],
    "homepage": "http://www.fork-cms.com/",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "facebook/php-sdk": "3.2.*",
        "tijsverkoyen/akismet": "1.1.*",
        "tijsverkoyen/css-to-inline-styles": "1.2.*",
        "matthiasmullie/minify": "1.1.*",
        "symfony/dependency-injection": "2.3.*",
        "symfony/http-kernel": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/yaml": "2.3.*",
        "symfony/filesystem": "2.3.*",
        "symfony/finder": "2.3.*",
        "spoon/library": "1.3.*",
        "symfony/monolog-bundle": "2.3.*",
		"google/google-api-php-client": "0.6.*"
    },
    "support": {
        "irc": "irc://irc.freenode.org/forkcms",
        "issues": "https://github.com/forkcms/forkcms/issues"
    },
    "scripts": {
        "post-update-cmd": "cd tools && ./remove_cache"
    },
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "google/google-api-php-client",
				"version": "0.6.2",
				"dist": {
					"url": "http://google-api-php-client.googlecode.com/files/google-api-php-client-0.6.2.tar.gz",
					"type": "tar"
				},
				"autoload": {
					"classmap": ["src/"]
				}
			}
		}
	]
}
